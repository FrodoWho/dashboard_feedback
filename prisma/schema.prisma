generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int        @id @default(autoincrement())
  email    String     @unique
  feedback Feedback[]
}

model Feedback {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  url       String
  problem   String
  comment   String?
  userId    Int
  user      User     @relation(fields: [userId], references: [id])

  @@index([userId], map: "Feedback_userId_fkey")
}
